{
  "expo": {
    "name": "Lunch Bucket",
    "description": "Online food delivery Application.",
    "slug": "lunch-bucket",
    "scheme": "com.icode.lunchbucket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash1.png",
      "waitForLoadingScreen": true,
      "loadingScreenImage": "./assets/splash1.png",
      "resizeMode": "cover",
      "backgroundColor": "#FCF0C8",
      "loadingScreenBackgroundColor": "#FCF0C8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#9E0808"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.icode.lunchbucket",
      "versionCode" : 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d93b88ed-359c-4e08-81fc-c9e65522372e"
      }
    }
  }
}